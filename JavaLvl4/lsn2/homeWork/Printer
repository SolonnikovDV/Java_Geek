package lsn2.homeWork;

public class Printer extends Product {

    private String color;
    private String type;

    public Printer(String _productType, int _model, String _color, String _type, String _maker, int _price) {
        super(_productType, _model, _price, _maker);
        this.color = _color;
        this.type = _type;
    }

    public String getColor() {
        return color;
    }

    public void setColor(String color) {
        this.color = color;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    @Override
    public String getProductType() {
        return super.getProductType();
    }
}
