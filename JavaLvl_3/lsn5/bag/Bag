package lsn5.HomeWorkDoing.Bag;

import java.util.*;

public class Bag {
    public List<Item> bag = new ArrayList<>();
    Item first;

    public Bag() {
        this.first = null;
    }

    // метод вывод размера списка
    public double size (){
        return bag.size();
    }

    // метод добавления предмета в лист пользоваеля
    public void insert(Item item) {
        item.next = first;
        first = item;
    }

    // метод вывода списка в консоль
    public void display() {
        Item current = first;
        while (current != null) {
            current.print();
            current = current.next;
        }
    }

}
