package lsn2.homeWork;

public class Product {
    private String productType;
    private int model;
    private int price;
    private String maker;

    public Product(String _productType, int _model, int _price, String _maker) {
        this.productType = _productType;
        this.model = _model;
        this.price = _price;
        this.maker = _maker;
    }

    public String getProductType() {
        return productType;
    }

    public void setProductType(String productType) {
        this.productType = productType;
    }

    public int getModel() {
        return model;
    }

    public void setModel(int model) {
        this.model = model;
    }

    public int getPrice() {
        return price;
    }

    public void setPrice(int price) {
        this.price = price;
    }

    public String getMaker() {
        return maker;
    }

    public void setMaker(String maker) {
        this.maker = maker;
    }
}
